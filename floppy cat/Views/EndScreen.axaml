<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="200"
        x:Class="floppy_cat.Views.EndScreen"
        Width="200"
        Height="200"
		SystemDecorations="BorderOnly"
        WindowStartupLocation="CenterScreen"
		SizeToContent="WidthAndHeight"
        Title="EndScreen">
        <Grid RowDefinitions="*, *">
                <Grid Grid.Row="0" ColumnDefinitions="*, *">
                        <TextBlock VerticalAlignment="Center" FontSize="18" FontWeight="Bold" Grid.Column="0">результат: </TextBlock>
                        <TextBlock x:Name="resText" Grid.Column="1"></TextBlock>
                </Grid>
                <Button Grid.Row="1" Width="150" Height="60 " VerticalAlignment="Center" HorizontalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="меню" x:Name="okButton"></Button>
        </Grid>
</Window>
